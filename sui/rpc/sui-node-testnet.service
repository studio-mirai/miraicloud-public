[Unit]
Description=sui-node-testnet
After=network.target

[Service]
User=mirai
Group=mirai
ExecStart=/opt/sui/testnet/bin/sui-node --config-path /opt/sui/testnet/config/config.yaml
Restart=always
RestartSec=5
LimitNOFILE=65535
CPUAffinity=10 26 11 27 12 28 13 29 14 30 15 31
MemoryMax=64G

[Install]
WantedBy=multi-user.target
